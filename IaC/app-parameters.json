{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "safemasks-app-portal"
        },
        "imageName": {
            "value": "safemasksacr.azurecr.io/safemasks:app"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "imageRegistryLoginServer": {
            "value": "safemasksacr.azurecr.io"
        },
        "imageUsername": {
            "value": "safemasksacr"
        },
        "imagePassword": {
            "value": null
        },
        "ports": {
            "value": [
                        {
                            "protocol": "tcp",
                            "port": "80"
                        },
                        {
                            "protocol": "tcp",
                            "port": "443"
                        }
                    ]
        },
          "dnsNameLabel": {
            "value": "safemasks-app-portal"
        },
        "ipAddressType": {
            "value": "Public"
        }
    }
}